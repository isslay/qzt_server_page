<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qzt.bus.dao.mapper.QztTimingLogMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.qzt.bus.model.QztTimingLog">
        <result column="id_" property="id" />
        <result column="create_by" property="createBy" />
        <result column="create_time" property="createTime" />
        <result column="update_by" property="updateBy" />
        <result column="update_time" property="updateTime" />
        <result column="bus_type" property="busType" />
        <result column="bus_id" property="busId" />
        <result column="result_enforcement" property="resultEnforcement" />
        <result column="bus_remark" property="busRemark" />
    </resultMap>


    <!-- 通用查询方法 -->
    <select id="find" resultMap="BaseResultMap" parameterType="map">
        SELECT * FROM dgm_qzt_timing_log
        <where>
            <if test="id != null and id != ''">
                AND id_ = #{id}
            </if>
        </where>
        ORDER BY create_time DESC
    </select>

</mapper>
