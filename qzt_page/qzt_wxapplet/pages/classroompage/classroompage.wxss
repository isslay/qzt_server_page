/* pages/classroompage/classroompage.wxss */
@import"../../utils/wxParse/wxParse.wxss";

.roompage{
  width: 100%;
  height: 100%;
  position: absolute;
  bottom: 0;
  left: 0;
  top: 0;
  right: 0;
  background: #fff;
  display: flex;
  flex-direction: column
}
.roomcontainer{
  flex: 1;
  padding: 20rpx 40rpx;
  overflow-y: scroll
}

.roomcontainer image{
  width: 100% !important;
  display: block;
  margin: 20rpx auto;
  border-radius: 20rpx
}


.roombtn{
  width: 100%;
  height: 100rpx;
  background: #22723C;
  line-height: 100rpx;
  text-align: center;
  color: #fff;
  font-size: 32rpx
}
.wxParse image{
  width: 100%
}