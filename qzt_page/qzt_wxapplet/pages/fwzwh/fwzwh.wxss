/* pages/fwzwh/fwzwh.wxss */

view {
  box-sizing: border-box;
}

.cooperationpage {
  width: 100%;
  height: 100%;
  padding-bottom: 250rpx;

}

.fwzwh {
  padding: 20rpx 40rpx;
  padding-bottom: 110rpx;
}

.logo {
  text-align: center;
  padding: 20rpx 0;
}

.cooimg {
  width: 390rpx;
  height: 42rpx;
}

.coolist1 {
  margin-top: 20rpx;
}

.coolist {
  margin-top: 40rpx;
}

.cooname {
  color: #565454;
  font-size: 30rpx;
  font-weight: bolder;
}

.cooinput {
  border-bottom: 1px solid #f4f4f4;
  padding: 10rpx 0;
  position: relative;
}

.cooinput input {
  font-size: 28rpx;
  color: #525252;
}
.cooinput textarea {
  font-size: 28rpx;
  color: #525252;
}

.textareavla{
  font-size: 28rpx;
  color: #525252;
}

.textSpan {
  font-size: 28rpx;
  color: #525252;
}

.btn {
  background: #22723c;
  height: 100rpx;
  font-size: 32rpx;
  text-align: center;
  line-height: 100rpx;
  color: #fff;
  position: fixed;
  width: 100%;
  bottom: 0;
  z-index: 99999999999
}

.whimg {
  width: 30rpx;
  height: 30rpx;
  position: absolute;
  right: 0;
  top: 18rpx;
}

button {
  background-color: rgb(247, 247, 247);
  border-radius: 0rpx;
  padding: 0rpx;
  margin: 0rpx;
  display: inline-block;
  line-height: 1;
}

button::after {
  /*button的边框样式是通过::after方式实现的*/
  border: none;
}

.button-hover {
  /*点击后样式*/
  color: #000;
  background-color: rgb(247, 247, 247);
}
.imglist{
  width:220rpx;
  float: left;
  padding: 10rpx;
  height: 220rpx;
  position: relative;
  z-index: 9999
}

.syimg {
 width: 200rpx;
 height: 200rpx;
 padding-right: 15rpx;
}

.time1 {
  float: left;
  width: 25%;
  font-size: 28rpx;
  color: #525252;

}

.time2 {
  font-size: 28rpx;
  color: #525252;
}
.delete{
  width: 30rpx;
  height: 30rpx;
  position: absolute;
  top: 20rpx;
  right: 20rpx
}
.syimg{
  width: 100%;
  height: 100%;
}
.syzptitlespan{
  float: right;
  font-size: 24rpx;
  color: #b10808
}