/* pages/myyhq/myyhq.wxss */

view {
  box-sizing: border-box;
}

.yhq {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  bottom: 0rpx;
  left: 0;
  right: 0;
  background: #f4f4f4;
  overflow: hidden;
  display: flex;
  flex-direction: column;
}

page {
  overflow: hidden;
}

.title {
  width: 100%;
  height: 90rpx;
  font-size: 28rpx;
  background: white;
  display: flex;
  align-items: center;
  justify-content: space-around;
  margin-top: 5rpx;
}

.textac {
  color: #22723c;
}

.titleSel {
  color: #a1a6bb;
  font-size: 28rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  height: 90rpx;
  line-height: 90rpx;
  position: relative;
}

.titleUnsel {
  color: #858fab;
  font-size: #858fab;
}

.headerLineSel {
  background: #22723c;
  height: 6rpx;
  width: 30rpx;
  position: absolute;
  bottom: 0px;
}

.headerLineUnsel {
  background: #fff;
  height: 0rpx;
  width: 100%;
  position: relative;
  margin-top: 10rpx;
}

.swiper {
  width: 100%;
  flex: 1;
  height: 150px;
  overflow: hidden;
  margin: 10rpx 0;
}

.yhqpage {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
}

.content {
  flex: 1;
  padding: 20rpx 40rpx;
  overflow-y: auto;
}

.btn {
  width: 100%;
  height: 120rpx;
  padding: 40rpx;
  padding-top: 5rpx;

}

.startPull {
  position: fixed;
  right: 60rpx;
  bottom: 100rpx;
}

.startPullimg {
  width: 120rpx;
  height: 120rpx;
}

.button {
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
  text-align: center;
  color: #fff;
  background: #22723c;
  font-size: 32rpx;
  border-radius: 10rpx;
}

.list {
  width: 100%;
  height: 190rpx;
  position: relative;
  display: flex;
  margin-bottom: 20rpx;
}

.backgroundimg {
  width: 100%;
  height: 190rpx;
  position: absolute;
  top: 0;
  left: 0;
}

.listleft {
  width: 190rpx;
  height: 100%;
  z-index: 9999;
  display: flex;
  color: #22723c;
  font-weight: bold;
  align-items: center;
  justify-content: center;
  font-size: 36rpx;
}

.listleft text {
  font-size: 64rpx;
}

.listleft span {
  margin-top: 20rpx;
}

.listright {
  flex: 1;
  height: 100%;
  z-index: 999;
  display: flex;
  flex-direction: column;
  padding: 20rpx;
}

.one {
  color: #ff9f01;
  font-size: 30rpx;
  flex: 1;
}

.two {
  font-size: 24rpx;
  color: #a1a6bb;
  flex: 1;
  margin-top: 23rpx;
}

.color {
  color: #a1a6bb;
}

.section_ {
  height: 84rpx;
  background: #fff;
  border-bottom: 1px solid #f4f4f4;
  line-height: 84rpx;
}

.section_title {
  display: inline-block;
  line-height: 85rpx;
  color: rgba(0, 0, 0, 1);
  font-size: 33rpx;
  float: left;
}

.section_title_picker {
  display: inline-block;
  font-size: 33rpx;
  color: #c7c7cc;
  width: 71%;
  margin-left: 36rpx;
  height: 75rpx;
  letter-spacing: 2rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}


.commodity_screen {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.5;
  overflow: hidden;
  z-index: 9;
  color: #fff;
}

.commodity_attr_box {
  width: 100%;
  overflow: hidden;
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 10;
  background: #ffffff;
  height:620rpx;
}
