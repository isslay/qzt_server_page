  display: flex
}
.raido{
  width: 70rpx;
  display: flex;
  align-items: center;
  justify-content: center
}
.raidopage{
  flex: 1;
   padding: 20rpx 20rpx 0 0rpx;
}
radio .wx-radio-input{ 
 /* 自定义样式.... */
  border-radius: 50%;
  width: 30rpx;
  height: 30rpx
 }

radio .wx-radio-input.wx-radio-input-checked {
  border-radius: 50%;
  border-color:#22723C !important;
  position: relative
}
radio .wx-radio-input.wx-radio-input-checked::before {
 content: ''; 
 width: 18rpx;
 height: 18rpx;
 border-radius: 50%;
 background-color: #22723C;
 top: 15rpx;
 display: flex; 
 left: 16rpx;
 position: relative;
}

.cklslist{
  border-bottom: 1px solid #f4f4f4;
  display: flex;
  padding: 20rpx 0;
  padding-top: 45rpx;

}

.ckpage{
  flex: 1
}
.name{
  font-size: 28rpx;
  color: #222328;
}
.time{
  font-size: 26rpx;
  color: #A1A6BB;
  margin-top: 20rpx;
}
.moneyadd{
  font-size: 26rpx;
  font-weight: bolder;
}
.scroll{
width: 100%;
height: 100%
}
