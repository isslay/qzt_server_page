/* 支付 */

.password-input {
  display: flex;
  width: 660rpx;
  border: 1px solid #ddd;
  margin: 0 auto;
  margin-top: 50rpx;
  background-color: #fff;
  border-radius: 3px;
}

.password-input view {
  width: 110rpx;
  height: 80rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  box-sizing: border-box;
  border-right: 1px solid #ccc;
}

.password-input view:nth-child(6) {
  border-right: none;
}

.password-input view text {
  width: 6px;
  height: 6px;
  border-radius: 50%;
  background-color: #333;
}

.tips {
  font-size: 28rpx;
  text-align: center;
  margin-top: 5px;
}

.tips text {
  color: #c30;
}

.keyboard {
  width: 100%;
  display: flex;
  background-color: #fff;
  border-top: 1px solid #eee;
  margin-top: 50rpx;
}

.keyboard .number {
  display: flex;
  width: 100%;
  flex-wrap: wrap;
}

.keyboard .number view {
  width: 33.3%;
  height: 120rpx;
  line-height: 110rpx;
  text-align: center;
  font-size: 46rpx;
  font-weight: bold;
  box-sizing: border-box;
  border-right: 1px solid #eee;
  border-bottom: 1px solid #eee;
}

.keyboard .ctr-btn {
  width: 180rpx;
}

.keyboard .ctr-btn view {
  height: 240rpx;
  line-height: 240rpx;
  text-align: center;
  box-sizing: border-box;
  border-bottom: 1px solid #eee;
}

.keyboard .ctr-btn view .iconfont {
  font-size: 44rpx !important;
  color: #c30;
}

.keyboard .ctr-btn view:nth-child(2) {
  font-size: 46rpx;
  color: #c30;
}

.keyboard .active {
  background-color: #e4e7ed;
}

.activity {
  background-color: #fff;
  padding: 25rpx 20rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 24rpx;
  border-bottom: 1px solid #f6f6f6;
}

.activity.activity-item {
  font-size: 26rpx;
}

.arrows-right {
  width: 30rpx;
  height: 30rpx;
}

.arrows-right.active {
  transform: rotate(270deg);
}

.loading {
  width: 80rpx;
  height: 80rpx;
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  z-index: 99999;
}

.loadingN {
  display: block;
  width: 80rpx;
  height: 80rpx;
  margin: 0 auto;
}

.network {
  width: 400rpx;
  height: 400rpx;
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  z-index: 999;
}

.networkN {
  display: block;
  width: 400rpx;
  height: 400rpx;
  margin: 0 auto;
  margin-top: 50rpx;
}

.fanhui {
  width: 70rpx;
  height: 70rpx;
  position: fixed;
  right: 30rpx;
  bottom: 50rpx;
  z-index: 999;
}

.fanhui image {
  display: block;
  width: 70rpx;
  height: 70rpx;
}

.wxParse-img {
  width: 690rpx;
}

.firmOrder_subtitle {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

/* 分享 */

.partakeBack {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: rgba(55, 63, 74, 0.70);
  z-index: 999999999;
}

.partake {
  width: 710rpx;
  height: 424rpx;
  background: rgba(255, 255, 255, 0.91);
  border-radius: 12rpx;
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  z-index: 999999999;
}

.partake_title {
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  font-size: 32rpx;
  color: #24272b;
  text-align: center;
}

.partake_subtitle {
  display: block;
  width: 100%;
  min-height: 40rpx;
  line-height: 40rpx;
  text-align: center;
  font-size: 32rpx;
  color: #24272b;
  margin-top: 50rpx;
}

.partake_but {
  width: 100%;
  height: 90rpx;
  margin-top: 90rpx;
}

.partake_but button {
  width: 310rpx;
  height: 90rpx;
  float: left;
  border-radius: 12rpx;
  margin: 0 23rpx;
  line-height: 90rpx;
  text-align: center;
  font-size: 32rpx;
  color: #fff;
}
