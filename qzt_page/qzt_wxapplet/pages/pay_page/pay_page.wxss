/* pages/pay_page/pay_page.wxss */

page {
  background: #f4f4f4;
}

.defray_box {
  width: 100%;
  height: auto;
  clear: both;
  overflow: hidden;
}

.defray_title {
  display: block;
  width: 100%;
  height: 42rpx;
  line-height: 42rpx;
  font-size: 30rpx;
  color: #222328;
  text-align: center;
  margin-top: 60rpx;
}

.defray_money {
  display: block;
  width: 100%;
  height: 71rpx;
  line-height: 71rpx;
  font-size: 64rpx;
  color: #24272b;
  font-weight: bold;
  text-align: center;
  margin-top: 30rpx;
}

.defray_collect {
  width: 100%;
  height: 120rpx;
  background: #fff;
  margin-top: 60rpx;
}

.defray_collect view {
  width: auto;
  height: 120rpx;
  float: left;
  line-height: 120rpx;
  margin-left: 30rpx;
  font-size: 30rpx;
  color: #222328;
  font-weight: bold;
}

.defray_collect text {
  display: block;
  width: auto;
  height: 120rpx;
  line-height: 120rpx;
  float: right;
  font-size: 28rpx;
  color: #24272b;
  margin-right: 30rpx;
}

.defray_but {
  width: 690rpx;
  height: 90rpx;
  background: #22723c;
  border-radius: 12rpx;
  line-height: 90rpx;
  text-align: center;
  font-size: 32rpx;
  color: #fff;
  margin: 0 auto;
  margin-top: 60rpx;
}

/* @import '../../utils/public.wxss'; */

.box {
  display: flex;
  flex-direction: column;
  height: 100%;
}

/* 主页导航 */

.homePage_nav {
  width: 100%;
  height: 100rpx;
  position: fixed;
  bottom: 0;
  left: 0;
  background: #fff;
  border-top: 1rpx solid #f4f4f4;
  z-index: 999;
}

.homePage_nav view {
  width: 25%;
  height: 100rpx;
  float: left;
}

.homePage_nav view image {
  display: block;
  width: 50rpx;
  height: 50rpx;
  margin: 0 auto;
  margin-top: 13rpx;
}

.homePage_nav view text {
  display: block;
  width: 100%;
  height: 30rpx;
  line-height: 30rpx;
  text-align: center;
  font-size: 22rpx;
  color: #666;
}

.goBack {
  width: 100%;
  height: 100rpx;
  flex-direction: row;
  position: fixed;
  top: 0;
  background: #fff;
  z-index: 999;
  border-bottom: 1px solid #f4f4f4;
}

.goBackTwo {
  width: 100%;
  height: 40rpx;
  position: relative;
  background: #fff;
}

.goBackTwo view {
  width: 150rpx;
  height: 40rpx;
  line-height: 40rpx;
  font-size: 24rpx;
  color: #2a2a2a;
  text-align: right;
  position: absolute;
  right: 30rpx;
}

.goBack image {
  width: 50rpx;
  height: 50rpx;
  position: absolute;
  top: 30rpx;
  left: 40rpx;
  z-index: 2;
}

.goBack text {
  width: 73%;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  font-size: 34rpx;
  color: #2a2a2a;
  font-weight: bold;
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.goBack view {
  width: 150rpx;
  height: 40rpx;
  line-height: 40rpx;
  font-size: 24rpx;
  color: #2a2a2a;
  text-align: right;
  position: absolute;
  top: 30rpx;
  right: 30rpx;
}

.goShai {
  display: block;
  width: 26rpx;
  height: 22rpx;
  text-align: right;
  position: absolute;
  top: 40rpx;
  right: 30rpx;
  background: url(http://img.rhzt.net/klyg112.png);
  background-size: 100%;
  z-index: 9999;
}

.nav_sub {
  width: 100%;
  height: 90rpx;
}

/* 四个按钮的选择按钮 */

.four_nav {
  width: 100%;
  height: 90rpx;
  background: #fff;
  position: fixed;
  top: 0;
  left: 0;
}

.four_nav view {
  width: 25%;
  height: 90rpx;
  float: left;
}

.four_nav view text {
  display: block;
  width: 100%;
  height: 82rpx;
  line-height: 82rpx;
  font-size: 28rpx;
  color: #a1a6bb;
  text-align: center;
}

.four_nav view i {
  width: 24rpx;
  height: 8rpx;
  margin: 0 auto;
  background: #ff553b;
  display: none;
}

.four_nav view.four_active text {
  color: #ff553b;
}

.four_nav view.four_active i {
  display: block;
}

